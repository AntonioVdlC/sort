{
  "name": "@antoniovdlc/sort",
  "version": "1.2.3",
  "description": "Custom compare functions for sorting arrays.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.cjs.js",
    "dist/index.esm.js",
    "dist/index.d.ts",
    "dist/types.d.ts",
    "dist/create-compare-fn.d.ts"
  ],
  "scripts": {
    "prepare": "husky install",
    "type:check": "tsc src/*.ts --noEmit",
    "format": "prettier --write --ignore-unknown {src,test}/*",
    "format:check": "prettier --check {src,test}/*",
    "test": "vitest run --coverage",
    "pre-commit": "lint-staged",
    "prebuild": "rimraf dist && mkdir dist",
    "build": "npm run build:types && npm run build:lib",
    "build:types": "tsc src/*.ts --declaration --emitDeclarationOnly --outDir dist",
    "build:lib": "rollup -c",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "sort",
    "array",
    "object",
    "compare"
  ],
  "author": "Antonio Villagra De La Cruz",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AntonioVdlC/sort.git"
  },
  "bugs": {
    "url": "https://github.com/AntonioVdlC/sort/issues"
  },
  "homepage": "https://github.com/AntonioVdlC/sort#readme",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "c8": "^7.11.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4",
    "vite": "^4.1.3",
    "vitest": "^0.9.3"
  },
  "dependencies": {
    "get-value-key": "^0.1.0"
  }
}
